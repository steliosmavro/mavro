{
    "name": "mavro",
    "private": true,
    "scripts": {
        "build": "turbo run build",
        "dev": "turbo run dev",
        "dev:mavrochat": "npm run dev --workspace=mavrochat",
        "lint": "turbo run lint",
        "format": "prettier --write \"**/*.{ts,tsx,md}\"",
        "check-types": "turbo run check-types"
    },
    "devDependencies": {
        "prettier": "^3.5.3",
        "turbo": "^2.5.4",
        "typescript": "5.8.2"
    },
    "engines": {
        "node": ">=18"
    },
    "packageManager": "npm@10.1.0",
    "workspaces": [
        "apps/*",
        "packages/*"
    ]
}
